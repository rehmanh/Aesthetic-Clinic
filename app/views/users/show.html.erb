<h2><%= @user.name %></h2>

<div>
  <h3>Email</h3>
  <p> <%= @user.email %> </p>
  <h3>Phone Number</h3>
  <p> <%= @user.phone_number %> </p>
  <h3>Complaint</h3>
  <p> <%= @user.complaint %> </p>
</div>

<p>
  <%= link_to "Edit Patient", edit_user_path %> |
  <%= link_to "Delete Patient", user_path(@user), method: :delete, data: {  confirm: 'Are you sure you want to delete this patient?' }  %>
</p>


