<div class="row">
  <div class="column">
  <h1>Sign In</h1>
  <%= form_tag(session_path) do %>
  <form>
    <ul>
      <%= label_tag :email %>
      <%= email_field_tag :email, nil, required: true %>
      <%= label_tag :password %>
      <%= password_field_tag :password, nil, required: true %>
      <%= submit_tag 'Sign In', class: 'button' %>
    </ul>
      <%#= link_to 'Cancel', nil, class: 'alert button' %>
  </form>
  <%end %>
  </div>
</div>
